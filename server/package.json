{
  "scripts": {
    "dev": "docker compose -f docker-compose.dev.yml up --build",
    "dev:local": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "dev:down": "docker compose  -f docker-compose.dev.yml down && docker compose -f docker-compose.dev.yml down -v",
    "down": "docker compose -f docker-compose.yml down && docker compose down -v",
    "db-dev:migrate-with-seed": "TS_NODE_PROJECT=./tsconfig.json npx tsx -r tsconfig-paths/register ./src/db/migrate-seed-db.ts",
    "db-prod:migrate-with-seed": " node  dist/db/migrate-seed-db.js",
    "build": "docker compose -f docker-compose.yml up --build",
    "build:prod": " tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push",
    "db:pull": "npx drizzle-kit pull",
    "db:up": "npx drizzle-kit up",
    "db:studio": "npx drizzle-kit studio"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/heic-convert": "^2.1.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/opossum": "^8.1.9",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@socket.io/redis-streams-adapter": "^0.2.2",
    "arctic": "^3.7.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "busboy": "^1.6.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "express": "^5.1.0",
    "ffprobe-static": "^3.1.0",
    "file-type": "^21.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "heic-convert": "^2.1.0",
    "ioredis": "^5.6.1",
